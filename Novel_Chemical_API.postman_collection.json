    {
      "name": "üß™ Chemical Analysis (CrewAI)",
      "item": [
        {
          "name": "Analyze with Prompt Only (Chatbot Mode)",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{auth_token}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"prompt\": \"Buatkan serum anti-aging untuk kulit sensitif\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{base_url}}/api/analyze",
              "host": ["{{base_url}}"],
              "path": ["api", "analyze"]
            },
            "description": "Pure chatbot mode - natural language input"
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "const response = pm.response.json();",
                  "if (response.conversation_id) {",
                  "  pm.environment.set('conversation_id', response.conversation_id);",
                  "  console.log('‚úÖ Conversation ID saved:', response.conversation_id);",
                  "}",
                  "console.log('‚è±Ô∏è Processing time:', response.processing_time + 's');",
                  "if (response.data) {",
                  "  console.log('üß™ Compound:', response.data.nama_senyawa);",
                  "  console.log('üìù Formula:', response.data.formula);",
                  "  console.log('üìä Recommendations:', response.data.recommendations?.length || 0);",
                  "}"
                ]
              }
            }
          ],
          "response": []
        },
